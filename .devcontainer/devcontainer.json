{
	"name": "Deno",
 	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "bullseye"
		}
	},
	"remoteUser": "vscode",
	"features": {
		"ghcr.io/devcontainers/features/desktop-lite:1": {
			"version": "latest"
		}
	},
	"runArgs": ["--init", "--shm-size=1g"],
	"forwardPorts": [6080, 5901],
	"overrideCommand": false,
	"portsAttributes": {
		"3000": {
			"label": "VERCEL"
		},
		"4507": {
			"label": "HTTP SERVER"
		},
		"5901": {
			"label": "VNC"
		},
		"6080": {
			"label": "WEB VNC"
		}
	}
}